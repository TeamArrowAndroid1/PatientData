<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="patient.arrow.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Arrow Health Care"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Search"
            android:parentActivityName=".Staff_Activity"/>
        <activity android:name=".Sign_In">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Staff_Activity"
            />
        <activity android:name=".Patient_activity"
            android:parentActivityName=".Staff_Activity"/>
        <activity android:name=".Add_Patient"
            android:parentActivityName=".Staff_Activity"></activity>
    </application>

</manifest>